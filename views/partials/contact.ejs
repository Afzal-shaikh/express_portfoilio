<main class="container p-5 pt-3">
    <div class="p-5 mb-3 home-wrapper">
        <h1 class="display-4 text-center fw-bold">Contact Me<br> 
            <!-- Panel with contact information-->

<!-- Twitter -->
<a class="btn btn-primary" style="background-color: #55acee;" href="#!" role="button"
  ><i class="fab fa-twitter"></i
></a>


<!-- Linkedin -->
<a class="btn btn-primary" style="background-color: #0082ca;" href="#!" role="button"
  ><i class="fab fa-linkedin-in"></i
></a>

<!-- Stack overflow -->
<a class="btn btn-primary" style="background-color: #ffac44;" href="#!" role="button"
  ><i class="fab fa-stack-overflow"></i
></a>


<!-- Github -->
<a class="btn btn-primary" style="background-color: #333333;" href="#!" role="button"
  ><i class="fab fa-github"></i
></a> <br>


    </div>


 <!-- Contact form -->
    <form class = "mb-5">
    <!-- First Name -->
     <div class="form-group mb-2">
    <label for="contactFirstName">First Name</label>
    <input type="text" class="form-control" id="contactFirstName" aria-describedby="contactFirstName" placeholder="First Name">
  </div>

    <!-- Last Name -->
     <div class="form-group mb-2">
    <label for="contactLastName">Last Name</label>
    <input type="text" class="form-control" id="contactLastName" aria-describedby="contactLastName" placeholder="Last Name">
  </div>

  <!-- Contact No. -->
     <div class="form-group mb-2">
    <label for="contactLastName">Contact No.</label>
    <input type="text" class="form-control" id="contactLastName" aria-describedby="contactLastName" placeholder="Contact No.">
  </div>

    <!-- Email -->
  <div class="form-group mb-2">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>

<!-- Message -->
  <div class="form-group mb-2">
    <label for="exampleFormControlTextarea1">Message</label>
    <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
  </div>
  
  <!-- Submit -->
  
  <a href="/home" class = "btn btn-primary">  
  Submit 
     </a>
</form>
</main>